{
  "name": "selenium-in-der-cloud-direkt-mit-javascript",
  "version": "1.0.0",
  "description": "Selenium in der Cloud direkt mit JavaScript",
  "keywords": [
    "Selenium",
    "NightwatchJS",
    "BrowserStack"
  ],
  "author": "Axel Springer Ideas Engineering GmbH",
